#include<iostream>
#include<vector>
using namespace std;
class Solution {
public:
    vector<int> searchRange(vector<int>& nums, int key) {

        int first=-1,last=-1;
        int st=0;
        int end= nums.size()-1;
        while(st<=end){
            int mid= st+(end-st)/2;
            if(key==nums[mid]){
                first=mid;
                end=mid-1;
            }
            else if(key<nums[mid]){
                end= mid-1;

            }
            else if(key>nums[mid]){
                st= mid+1;
            }
        

            
        }
            st = 0;
         end = nums.size()-1;

    
        while(st<=end){
            int mid = st + (end-st)/2;

            if(nums[mid] == key){
                last = mid;
                st = mid + 1;
            }
            else if(nums[mid] < key){
                st = mid + 1;
            }
            else{
                end = mid - 1;
            }
        }

        return {first,last};
    }
};




        
    
